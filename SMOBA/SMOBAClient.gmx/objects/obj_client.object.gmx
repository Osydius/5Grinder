var distance_y

if mouse_check_button(mb_right)
{
    destination_x = mouse_x
    destination_y = mouse_y
    adjacent = abs(player_x - destination_x);
    opposite = abs(player_y - destination_y);
    
    if(adjacent != 0){
        angle = arctan(opposite/adjacent);
    } else {
        angle = 0;
    }
    
    image_angle = point_direction(player_x,player_y,destination_x,destination_y) - 90;
    ratio_x = floor(cos(angle)*player_speed);
    ratio_y = floor(sin(angle)*player_speed);
    
}

if player_x &gt; destination_x
{
    repeat(ratio_x){
        if (player_x != destination_x) and place_free(player_x - 1, player_y){
            player_x -= 1
        }
    }
}
else
{
repeat(ratio_x)
    {
    if (player_x != destination_x)and place_free(player_x + 1, player_y)
    
    {
        player_x += 1
    }
    }
}
if (player_y &gt; destination_y)
{
    repeat(ratio_y)
    {
    if (player_y != destination_y) and place_free(player_x , player_y - 1)
    
    {
        player_y -= 1
    }
    }
}
else
{
repeat(ratio_y)
    {
    if (player_y != destination_y)and place_free(player_x , player_y + 1)
    
    {
        player_y += 1
    }
    }
}
