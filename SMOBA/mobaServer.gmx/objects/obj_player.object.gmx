            <string>
// change direction of player
player_direction = point_direction(player_x,player_y,player_destination_x,player_destination_y) - 90;

// move the player
adjacent = abs(player_x - player_destination_x);
opposite = abs(player_y - player_destination_y);

if(adjacent != 0){
    player_angle = arctan(opposite/adjacent);
} else {
    player_angle = 0;
}
ratio_x = floor(cos(player_angle)*player_speed);
ratio_y = floor(sin(player_angle)*player_speed);

// move the player horizontally
if player_x &gt; player_destination_x{
    repeat(ratio_x){
        if (player_x != player_destination_x) and place_free(player_x - 1, player_y){
            player_x -= 1
        }
    }
} else {
    repeat(ratio_x){
        if (player_x != player_destination_x)and place_free(player_x + 1, player_y){
            player_x += 1
        }
    }
}

//move the player vertically
if (player_y &gt; player_destination_y){
    repeat(ratio_y){
        if (player_y != player_destination_y) and place_free(player_x , player_y - 1){
            player_y -= 1
        }
    }
} else {
    repeat(ratio_y){
        if (player_y != player_destination_y)and place_free(player_x , player_y + 1){
            player_y += 1
        }
    }
}

//collisions with bullets
</string>
